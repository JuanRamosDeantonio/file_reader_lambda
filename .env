# Configuración de AWS
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=tu_access_key_aqui
AWS_SECRET_ACCESS_KEY=tu_secret_key_aqui

# Configuración del File Reader
OUTPUT_FORMAT=markdown_ai
AI_OPTIMIZED=true
INCLUDE_METADATA=true
EXTRACT_KEY_SECTIONS=true
MAX_CHUNK_SIZE=4000

# Configuración de Logging (opcional)
LOG_LEVEL=INFO

# Configuración de Desarrollo (opcional)
DEVELOPMENT_MODE=false

# Copia este archivo a .env y actualiza con tus valores reales
# ¡Nunca hagas commit del archivo .env al control de versiones!